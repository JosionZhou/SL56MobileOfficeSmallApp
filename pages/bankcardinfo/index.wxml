<view class="weui-toptips weui-toptips_warn" wx:if="{{IsError}}">{{ErrorMessage}}</view>
<view class="weui-cells__title">请输入银行卡信息</view>
<form bindsubmit="submit">
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">银行卡号码</view>
      </view>
      <view class="weui-cell__bd">
        <input class="weui-input" name="ObjectName" placeholder="请输入银行卡号码" model:value="{{BankAcc}}" />
      </view>
    </view>
    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">银行名称</view>
      </view>
      <view class="weui-cell__bd">
        <input class="weui-input" name="ObjectName" placeholder="请输入银行名称" model:value="{{BankName}}" />
      </view>
    </view>
    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">开户名</view>
      </view>
      <view class="weui-cell__bd">
        <input class="weui-input" name="ObjectName" placeholder="请输入银行卡开户名" model:value="{{Name}}" />
      </view>
    </view>
  </view>
  <button class="weui-btn" type="primary" form-type="submit">提交</button>
</form>