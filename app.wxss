/**app.wxss**/
@import 'style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.weui-grids{
  border: none
}
.weui-grid {
  border: none
}
.weui-grid__label{
  margin-top: 10px;
}
.weui-grid_active1{
  background-color: #014a98
}
.weui-grid_active2{
  background-color: #f2f2f2
}
.floatLeft{
  float: left;
}
.floatRight{
  float:right;
}